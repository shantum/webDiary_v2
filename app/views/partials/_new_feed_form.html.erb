<%= form_tag('/user_feeds/create', method: 'post', class: "form-inline") do %>
    <%= text_field_tag(:url, nil, class: 'form-control', placeholder: 'Enter Feed URL here', type: 'url', id: 'user_feeds_new_input') %>
    <%= submit_tag('Save', class: 'btn btn-primary', id: 'user_feeds_new_button') %>
<% end %>